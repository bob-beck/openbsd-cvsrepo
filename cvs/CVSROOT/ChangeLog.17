    N ports/mail/vmailmgr/patches/patch-authenticate_Makefile_in
    N ports/mail/vmailmgr/pkg/COMMENT
    N ports/mail/vmailmgr/pkg/PLIST
    N ports/mail/vmailmgr/pkg/MESSAGE
    N ports/mail/vmailmgr/pkg/DESCR
    
