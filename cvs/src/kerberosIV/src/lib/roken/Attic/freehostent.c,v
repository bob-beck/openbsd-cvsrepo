#ifdef HAVE_CONFIG_H
#include <config.h>
RCSID("$KTH: freehostent.c,v 1.2 1999/12/02 16:58:46 joda Exp $");
#endif

#include "roken.h"

/*
 * free a malloced hostent
 */

void
freehostent (struct hostent *h)
{
    char **p;

    free (h->h_name);
    if (h->h_aliases != NULL) {
	for (p = h->h_aliases; *p != NULL; ++p)
	    free (*p);
	free (h->h_aliases);
    }
    if (h->h_addr_list != NULL) {
	for (p = h->h_addr_list; *p != NULL; ++p)
	    free (*p);
	free (h->h_addr_list);
    }
    free (h);
}
@


