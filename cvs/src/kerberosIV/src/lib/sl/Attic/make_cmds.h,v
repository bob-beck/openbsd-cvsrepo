/* $KTH: make_cmds.h,v 1.2 1999/12/02 16:58:55 joda Exp $ */

#ifndef __MAKE_CMDS_H__
#define __MAKE_CMDS_H__

#ifdef HAVE_CONFIG_H
#include <config.h>
#endif

#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <stdarg.h>

extern char *filename;
extern char *table_name;
extern int numerror;

struct command_list {
    char *function;
    char *help;
    struct string_list *aliases;
    unsigned flags;
    struct command_list *next;
    struct command_list **tail;
};

struct string_list {
    char *string;
    struct string_list *next;
    struct string_list **tail;
};

void add_command(char*, char*, struct string_list*, unsigned);

#endif /* __MAKE_CMDS_H__ */
@


