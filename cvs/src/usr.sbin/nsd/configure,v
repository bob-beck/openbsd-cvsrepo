if (eval "$ac_cpp conftest.$ac_ext") 2>&5 |
  $EGREP "(^|[^a-zA-Z_0-9])uintptr_t[^a-zA-Z_0-9]" >/dev/null 2>&1; then :
  ac_cv_type_uintptr_t=yes
