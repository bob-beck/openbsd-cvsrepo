#include <fcntl.h>

int
main ()
{
  char *f = "conftest.chown";
  struct stat before, after;

  if (creat (f, 0600) < 0)
    exit (1);
  if (stat (f, &before) < 0)
    exit (1);
  if (chown (f, (uid_t) -1, (gid_t) -1) == -1)
    exit (1);
  if (stat (f, &after) < 0)
    exit (1);
  exit ((before.st_uid == after.st_uid
	 && before.st_gid == after.st_gid) ? 0 : 1);

  ;
  return 0;
}
