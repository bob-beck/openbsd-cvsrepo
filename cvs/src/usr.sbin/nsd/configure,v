if (!res) return 2;
res = strptime("20070207111842", "%Y%m%d%H%M%S", &tm);
if (!res) return 1; return 0; }

_ACEOF
if ac_fn_c_try_run "$LINENO"; then :
  eval "ac_cv_c_strptime_works=yes"
else
  eval "ac_cv_c_strptime_works=no"
