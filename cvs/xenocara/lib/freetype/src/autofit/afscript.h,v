  SCRIPT( hani, HANI, "CJKV ideographs" )
  SCRIPT( hebr, HEBR, "Hebrew" )
  SCRIPT( latn, LATN, "Latin" )
#ifdef FT_OPTION_AUTOFIT2
  SCRIPT( ltn2, LTN2, "Latin 2" )
#endif
@

