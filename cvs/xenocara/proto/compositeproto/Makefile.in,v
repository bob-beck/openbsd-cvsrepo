	@@$(NORMAL_UNINSTALL)
	@@list='$(composite_HEADERS)'; test -n "$(compositedir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(compositedir)' && rm -f" $$files ")"; \
